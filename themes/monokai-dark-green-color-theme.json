	"tokenColors": [
		{
			"settings": {
				// "background": "#1A1A1A",
				"foreground": "#CFBFAD"
			}
		},
		{
			"name": "JavaScript - DOM & invocation color fix",
			"scope": "meta.function-call.js entity.name.function, meta.function-call.js support.function.dom.js, support.variable.dom.js, support.variable.property.js",
			"settings": {
				"foreground": "#66D9EF"
			}
		},
		{
			"name": "JavaScript - if object property fix",
			"scope": "support.variable.property.process",
			"settings": {
				"foreground": "#66D9EF"
			}
		},
		{
			"name": "JavaScript - if object fix",
			"scope": "support.variable.object.process",
			"settings": {
				"foreground": "#66D9EF",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#A0B6FF"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#3BEC8D"
			}
		},
