		"terminal.ansiBrightCyan": "#66D9EF",
		"terminal.ansiBrightWhite": "#CFBFAD",
		"breadcrumb.foreground": "#3C3F41",
		"editorBracketMatch.background": "#A6E22E",
		"editorBracketMatch.border": "#00000000",
